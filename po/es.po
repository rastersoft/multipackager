#: ../src/multipackager.py:65
msgid ""
"\n"
"This program must be run as root\n"
msgstr ""
"\n"
"Este programa tiene que ejecutarse como root\n"

#: ../src/multipackager.py:77
msgid "Distro name {:s} unknown. Aborting."
msgstr "Nombre de distribución {:s} desconocido. Abortando."

#: ../src/multipackager.py:107
msgid "Can't get the package name"
msgstr "Imposible obtener el nombre del paquete"

#: ../src/multipackager.py:116
msgid "Error when creating the working environment"
msgstr "Error al crear el entorno de trabajo"

#: ../src/multipackager.py:122
msgid "Failed to build the packages"
msgstr "No se han podido generar los paquetes"

#: ../src/multipackager.py:130
msgid "Built packages:"
msgstr "Paquetes generados:"

#: ../src/multipackager.py:134
msgid "Built packages: None"
msgstr "Paquetes generados: Ninguno"

#: ../src/multipackager.py:136
msgid "Skipped packages:"
msgstr "Paquetes omitidos:"

#: ../src/multipackager.py:140
msgid "Skipped packages: none"
msgstr "Paquetes omitidos: ninguno"

#: ../src/multipackager.py:157
msgid "The specified CHROOT environment at {:s} doesn't exists. Aborting."
msgstr "El entorno CHROOT especificado en {:s} no existe. Abortando."

#: ../src/multipackager.py:175
msgid "The project folder exists; launching the shell without copying data"
msgstr ""
"La carpeta de proyecto ya existe; lanzando la shell sin copiar los datos"

