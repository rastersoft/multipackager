msgstr "Creando una copia de {:s}"

#: ../src/multipackager_module/package_base.py:87
msgid "Updating {:s}"
msgstr "Actualizando {:s}"

#: ../src/multipackager_module/package_base.py:115
#: ../src/multipackager_module/package_base.py:135
msgid "Creating working copy of {:s}"
msgstr "Creando una copia de {:s}"

#: ../src/multipackager_module/package_base.py:194
msgid "Unknown build system"
msgstr "Sistema de compilación desconocido"

#: ../src/multipackager_module/package_base.py:288
msgid "Launching {:s}"
msgstr "Lanzando {:s}"

#: ../src/multipackager_module/debian.py:152
